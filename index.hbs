<html>
<head>
  <meta charset='utf-8' />
  <title>{{options.name}} {{options.version}}</title>
  <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700|Source+Code+Pro:400,500,700' rel='stylesheet' type='text/css'>
  <link href='assets/bass.css' type='text/css' rel='stylesheet' />
  <link href='assets/style.css' type='text/css' rel='stylesheet' />
  <link href='assets/github.css' type='text/css' rel='stylesheet' />
</head>
<body class='documentation'>
  <div class='container'>
    <div class='clearfix mxn2'>
      <div class='px2 py2 col sm-col-3'>
        <div class='fixed overflow-auto max-height-100'>
          <a href='/' class='h5 button block button-transparent'>{{options.name}} {{options.version}}</a>
          <div class='px2'>
            {{#each docs}}
              <a
                href='#{{permalink}}'
                class='block bold'>
                {{name}}
              </a>
              {{#each members.static}}
                <a
                  href='#{{permalink}}'
                  class='regular block'>
                  .{{name}}
                </a>
              {{/each}}
              {{#each members.instance}}
                <a
                  href='#{{permalink}}'
                  class='regular block'>
                  #{{name}}
                </a>
              {{/each}}
              {{#each events}}
                <a
                  href='#{{permalink}}'
                  class='regular block'>
                  <code>â“” {{name}}</code>
                </a>
              {{/each}}
            {{/each}}
          </div>
        </div>
      </div>
      <div class='col sm-col-9'>
        <div class='px1'>
{{#each docs ~}}{{> section}}{{/each}}
        </div>
      </div>
    </div>
  </div>
  <script src='assets/anchor.js'></script>
  <script>
  anchors.options.placement = 'left';
  anchors.add();
  </script>
</body>
</html>
